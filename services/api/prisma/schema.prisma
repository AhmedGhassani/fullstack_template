generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native", "debian-openssl-3.0.x", "linux-musl-openssl-3.0.x"]
}

datasource db {
    provider = "postgresql"
    url      = env("API_DB_URL")
}

model User {
    id        String   @id @default(uuid())
    firstName String
    lastName  String
    email     String   @unique
    password  String
    dob       DateTime
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
